{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./app/*"
      ],
      "@screens/*": [
        "./app/screens/*"
      ],
      "@components/*": [
        "./app/components/*"
      ],
      "@hooks/*": [
        "./app/hooks/*"
      ]
    }
  }
}
